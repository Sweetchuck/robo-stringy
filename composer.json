{
    "type": "robo-tasks",
    "name": "sweetchuck/robo-stringy",
    "description": "Exposes Stringy string manipulation methods",
    "keywords": [
        "robo-tasks",
        "stringy"
    ],
    "authors": [
        {
            "name": "DÃ¡vid Andor",
            "role": "Maintainer",
            "homepage": "https://github.com/Sweetchuck"
        }
    ],
    "support": {
        "source": "https://github.com/Sweetchuck/robo-stringy",
        "issues": "https://github.com/Sweetchuck/robo-stringy/issues"
    },
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "bin-dir": "bin",
        "sort-packages": true
    },
    "require": {
        "php": ">=7.2",
        "consolidation/robo": "^2.0",
        "danielstjules/stringy": "^3.1"
    },
    "require-dev": {
        "codeception/codeception": "^4.0",
        "codeception/module-asserts": "^1.1",
        "squizlabs/php_codesniffer": "^3.5",
        "sweetchuck/codeception-module-robo-task-runner": "^0.7",
        "sweetchuck/git-hooks": "^0.0",
        "sweetchuck/robo-git": "^0.2",
        "sweetchuck/robo-phpcs": "^0.1",
        "symfony/error-handler": "^5.0",
        "symfony/finder": "^4.0",
        "symfony/yaml": "^4.0",
        "webmozart/path-util": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Sweetchuck\\Robo\\Stringy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sweetchuck\\Robo\\Stringy\\Composer\\": "src-dev/Composer/",
            "Sweetchuck\\Robo\\Stringy\\Test\\Helper\\": "tests/_support/Helper/",
            "Sweetchuck\\Robo\\Stringy\\Tests\\Acceptance\\": "tests/acceptance/",
            "Sweetchuck\\Robo\\Stringy\\Tests\\Unit\\": "tests/unit/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "\\Sweetchuck\\Robo\\Stringy\\Composer\\Scripts::postInstallCmd"
        ],
        "post-update-cmd": [
            "\\Sweetchuck\\Robo\\Stringy\\Composer\\Scripts::postUpdateCmd"
        ]
    }
}
