{
    "type": "robo-tasks",
    "name": "sweetchuck/robo-stringy",
    "description": "Exposes Stringy string manipulation methods",
    "keywords": [
        "robo-tasks",
        "stringy"
    ],
    "authors": [
        {
            "name": "Andor",
            "role": "Maintainer",
            "homepage": "https://github.com/Sweetchuck"
        }
    ],
    "support": {
        "source": "https://github.com/Sweetchuck/robo-stringy",
        "issues": "https://github.com/Sweetchuck/robo-stringy/issues"
    },
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "sweetchuck/git-hooks": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "repositories": {},
    "require": {
        "php": ">=7.4",
        "consolidation/robo": "^3.0",
        "danielstjules/stringy": "^3.1"
    },
    "require-dev": {
        "ext-json": "*",
        "codeception/codeception": "^4.0",
        "codeception/module-asserts": "^1.1",
        "nuvoleweb/robo-config": "2.x-dev",
        "psr/container": "^1.0",
        "squizlabs/php_codesniffer": "^3.5",
        "sweetchuck/codeception-module-robo-task-runner": "2.x-dev",
        "sweetchuck/git-hooks": "2.x-dev",
        "sweetchuck/robo-git": "2.x-dev",
        "sweetchuck/robo-phpcs": "2.x-dev",
        "sweetchuck/utils": "1.x-dev",
        "symfony/error-handler": "^5.0 || ^6.0",
        "symfony/finder": "^5.0 || ^6.0",
        "symfony/yaml": "^5.0 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "Sweetchuck\\Robo\\Stringy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sweetchuck\\Robo\\Stringy\\Tests\\Acceptance\\": "tests/acceptance/",
            "Sweetchuck\\Robo\\Stringy\\Tests\\Unit\\": "tests/unit/",
            "Sweetchuck\\Robo\\Stringy\\Tests\\": "tests/_support/"
        }
    },
    "scripts": {},
    "scripts-descriptions": {},
    "extra": {}
}
